    <profile book="Codex: Necrons" hidden="false" id="5d30-6e84-6148-3610" name="Phaeron of the Sautekh Dynasty" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" profileTypeName="Abilities">
