  <profile hidden="false" id="e99a-f4d0-8e6a-8cea" name="Zealot" page="90" publicationId="d124-b283-pubN72234" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
   <characteristics>
    <characteristic name="Description" typeId="21befb24-fc85-4f52-a745-64b2e48f8228">
