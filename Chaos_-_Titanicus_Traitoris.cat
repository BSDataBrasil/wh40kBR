   </characteristics>
  </profile>
  <profile hidden="false" id="9ff7-ac90-8912-3833" name="Cataclysmic Explosion" profileTypeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" profileTypeName="Abilities">
